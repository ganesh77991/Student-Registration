<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ServletController</servlet-name>
  	<display-name>ServletController</display-name>
  	<description></description>
  	<servlet-class>com.student.controller.ServletController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterFormServlet</servlet-name>
  	<display-name>RegisterFormServlet</display-name>
  	<description></description>
  	<servlet-class>com.student.controller.RegisterFormServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DataServlet</servlet-name>
  	<display-name>DataServlet</display-name>
  	<description></description>
  	<servlet-class>com.student.controller.DataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServletController</servlet-name>
  	<url-pattern>/ServletController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterFormServlet</servlet-name>
  	<url-pattern>/RegisterFormServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DataServlet</servlet-name>
  	<url-pattern>/DataServlet</url-pattern>
  </servlet-mapping>  
</web-app>
